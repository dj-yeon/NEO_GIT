<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hdh.dao.IFinanceDao">
	<insert id="insertFinance" parameterType="com.hdh.vo.FinanceVo">
		INSERT INTO financial_statement
				(
				DATE,
				ID,
				businessLicenseNumber,
				PROPERTY,
				LIQUID_ASSETS,
				PARTY_PROPERTY,
				CASH_CASHABLE_ASSETS,
				OTHER_CASH_DEPOSITS,
				SHORT_TERM_DEPOSIT,
				TRADE_BOND,
				STOCK_ASSETS,
				NON_CURRENT_ASSETS,
				TANGIBLE_ASSETS,
				MACHINE_EQUIPMENT,
				INTANGIBLE_PROPERTY,
				DEVELOPMENT_COST,
				DEBT,
				CURRENT_LIABILITIES,
				SHORT_TERM_BORROWINGS,
				PURCHASE_DEBT,
				NON_CURRENT_LIABILITY,
				LONG_TERM_LOANS,
				EQUITY_CAPITAL,
				CAPITAL,
				RETAINED_EARNINGS_ARREARS,
				SALES,
				PRODUCT_SALES,
				WARE_SALES,
				OTHER_SALES,
				SALES_COST,
				GROSS_PROFIT_SALES,
				SALES_ADMINISTRATIVE_EXPENSES,
				R_D_EXPENSES,
				OPERATING_PROFIT,
				NON_OPERATING_EXPENSES,
				INTEREST_EXPENSE,
				NET_INCOME_BEFORE_TAX,
				CORPORATE_TAX_EXPENSE,
				NET_INCOME_TAX,
				MANUFACTURING_COST_R_D_EXPENSES,
				TOTAL_R_D_EXPENSES)
				VALUES
				(
				#{date},
				#{id},
				#{businessLicenseNumber},
				#{property},
				#{liquidAssets},
				#{partyProperty},
				#{cashCashableAssets},
				#{otherCashDeposits},
				#{shortTermDeposit},
				#{tradeBond},
				#{stockAssets},
				#{nonCurrentAssets},
				#{tangibleAssets},
				#{machineEquipment},
				#{intangibleProperty},
				#{developmentCost},
				#{debt},
				#{currentLiabilities},
				#{shortTermBorrowings},
				#{purchaseDebt},
				#{nonCurrentLiability},
				#{longTermLoans},
				#{equityCapital},
				#{capital},
				#{retainedEarningsArrears},
				#{sales},
				#{productSales},
				#{wareSales},
				#{otherSales},
				#{salesCost},
				#{grossProfitSales},
				#{salesAdministrativeExpenses},
				#{rDExpenses},
				#{operatingProfit},
				#{nonOperatingExpenses},
				#{interestExpense},
				#{netIncomeBeforeTax},
				#{corporateTaxExpense},
				#{netIncomeTax},
				#{manufacturingCostRDExpenses},
				#{totalRDExpenses}
				)
	</insert>
	<insert id="insertCFinance" parameterType="com.hdh.vo.CalculationVo">
		INSERT INTO financial_party_calculation
				(
					DATE,
					ID,
					businessLicenseNumber,
					EQUITY_CAPITAL_RATIO,
					FLOW_RATE,
					CHECKING_RATIO,
					DEBT_RATIO,
					BORROWING_DEPENDENCE,
					SHORT_TERM_BORROWING_RATIO,
					CASH_SECURITIES_RATIO,
					AVERAGE_LOAN_INTEREST_RATE,
					R_D_RATIO,
					BORROWING_SALES,
					FINANCIAL_EXPENSES_VS_LIABILITIES,
					INTEREST_COMPENSATION_MULTIPLE,
					RETURN_ASSETS,
					EARNINGS_RATIO_SALES_OPERATION,
					CONTINUED_BUSINESS_PROFIT_RATIO_BEFORE_SALES_TAX,
					NET_PROFIT_RATIO_SALES,
					FINANCIAL_EXPENSE_SALES_RATIO,
					INTEREST_COVERAGE_RATIO,
					RETURN_EQUITY,
					CAPITAL_TURNOVER,
					TURNOVER_RATE_ACCOUNTS_RECEIVABLE,
					INVENTORY_ASSET_TURNOVER_RATE,
					TURNOVER_RATE_TANGIBLE_ASSETS,
					TURNOVER_RATE_PURCHASE_DEBT,
					INCREASE_SALES,
					NET_INCOME_GROWTH_RATE,
					ASSET_GROWTH_RATE,
					GROWTH_RATE_EQUITY,
					PROPERTY_PLANT_EQUIPMENT_GROWTH_RATE
				)
				VALUES
				(
					#{date},
					#{id},
					#{businessLicenseNumber},
					#{equityCapitalRatio},
					#{flowRate},
					#{checkingRatio},
					#{debtRatio},
					#{borrowingDependence},
					#{shortTermBorrowingRatio},
					#{cashSecuritiesRatio},
					#{averageLoanInterestRate},
					#{rDRatio},
					#{borrowingSales},
					#{financialExpensesVsLiabilities},
					#{interestCompensationMultiple},
					#{returnAssets},
					#{earningsRatioSalesOperation},
					#{continuedBusinessProfitRatioBeforeSalesTax},
					#{netProfitRatioSales},
					#{financialExpenseSalesRatio},
					#{interestCoverageRatio},
					#{returnEquity},
					#{capitalTurnover},
					#{turnoverRateAccountsReceivable},
					#{inventoryAssetTurnoverRate},
					#{turnoverRateTangibleAssets},
					#{turnoverRatePurchaseDebt},
					#{increaseSales},
					#{netIncomeGrowthRate},
					#{assetGrowthRate},
					#{growthRateEquity},
					#{propertyPlantEquipmentGrowthRate}
				)
	</insert>
	<insert id="insertcorporateManagementAnalysis" parameterType="com.hdh.vo.ExcelVo">
		INSERT INTO CORPORATE_MANAGEMENT_ANALYSIS(
			industrialClassificationCode,
			majorCommodity,
			increaseSales,
			earningsRatioSalesOperation,
			equityCapitalRatio,
			interestCoverageRatio,
			turnoverRateAccountsReceivable
		)
		VALUE
		(
			#{industrialClassificationCode       },
			#{majorCommodity                     },
			#{increaseSales                      },
			#{earningsRatioSalesOperation        },
			#{equityCapitalRatio                 },
			#{interestCoverageRatio              },
			#{turnoverRateAccountsReceivable     }
		)
	</insert>
	<delete id="deletecorporateManagementAnalysis">
		DELETE FROM CORPORATE_MANAGEMENT_ANALYSIS
	</delete>
</mapper>